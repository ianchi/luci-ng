{
  "widget": "set-subpage",
  "if": "uciLoad('network') && (schema = loadSchemaExt('network', 'proto', uci.network['@interface'][parseInt($routeParam.id)].proto))",

  "content": [
    {
      "widget": "set-section",
      "options": {
        "title=": "`Interface ${uci.network['@interface'][parseInt($routeParam.id)]['.name'].toUpperCase()}`"
      },
      "content": ["uciUI('network','@interface', parseInt($routeParam.id))"]
    },

    "schema ? buildUI( schema, `uci.network['@interface'][parseInt($routeParam.id)]`) : []"
  ]
}
